{"pages" : [ 
{ "chapter" : 56 , "verse" : 77 , "ar" : "إِنَّهُۥ لَقُرۡءَانࣱ كَرِیمࣱ" , "tr" : "እርሱ የከበረ ቁርኣን ነው፡፡" },
{ "chapter" : 56 , "verse" : 78 , "ar" : "فِی كِتَـٰبࣲ مَّكۡنُونࣲ" , "tr" : "በተጠበቀ መጽሐፍ ውስጥ ነው፡፡" },
{ "chapter" : 56 , "verse" : 79 , "ar" : "لَّا یَمَسُّهُۥۤ إِلَّا ٱلۡمُطَهَّرُونَ" , "tr" : "የተጥራሩት እንጅ ሌላ አይነካውም፡፡" },
{ "chapter" : 56 , "verse" : 80 , "ar" : "تَنزِیلࣱ مِّن رَّبِّ ٱلۡعَـٰلَمِینَ" , "tr" : "ከዓለማት ጌታ የተወረደ ነው፡፡" },
{ "chapter" : 56 , "verse" : 81 , "ar" : "أَفَبِهَـٰذَا ٱلۡحَدِیثِ أَنتُم مُّدۡهِنُونَ" , "tr" : "በዚህ ንግግር እናንተ ቸልተኞች ናችሁን" },
{ "chapter" : 56 , "verse" : 82 , "ar" : "وَتَجۡعَلُونَ رِزۡقَكُمۡ أَنَّكُمۡ تُكَذِّبُونَ" , "tr" : "ሲሳያችሁንም (ዝናምን) እናንተ የምታስተባብሉት ታደርጋላችሁን" },
{ "chapter" : 56 , "verse" : 83 , "ar" : "فَلَوۡلَاۤ إِذَا بَلَغَتِ ٱلۡحُلۡقُومَ" , "tr" : "(ነፍስ) ጉረሮንም በደረሰች ጊዜ፡፡" },
{ "chapter" : 56 , "verse" : 84 , "ar" : "وَأَنتُمۡ حِینَئِذࣲ تَنظُرُونَ" , "tr" : "እናንተ ያን ጊዜ የምታዩ ስትኾኑ፡፡" },
{ "chapter" : 56 , "verse" : 85 , "ar" : "وَنَحۡنُ أَقۡرَبُ إِلَیۡهِ مِنكُمۡ وَلَـٰكِن لَّا تُبۡصِرُونَ" , "tr" : "እኛም ግን አታዩም እንጅ ከእናንተ ይልቅ ወደእርሱ የቀረብን ስንኾን፡፡" },
{ "chapter" : 56 , "verse" : 86 , "ar" : "فَلَوۡلَاۤ إِن كُنتُمۡ غَیۡرَ مَدِینِینَ" , "tr" : "የማትዳኙም ከኾናችሁ፡፡" },
{ "chapter" : 56 , "verse" : 87 , "ar" : "تَرۡجِعُونَهَاۤ إِن كُنتُمۡ صَـٰدِقِینَ" , "tr" : "እውነተኞች እንደ ኾናችሁ (ነፍሲቱን ወደ አካሉ) ለምን አትመልሷትም፡፡" },
{ "chapter" : 56 , "verse" : 88 , "ar" : "فَأَمَّاۤ إِن كَانَ مِنَ ٱلۡمُقَرَّبِینَ" , "tr" : "(ሟቹ) ከባለሟሎቹ ቢኾንማ፡፡" },
{ "chapter" : 56 , "verse" : 89 , "ar" : "فَرَوۡحࣱ وَرَیۡحَانࣱ وَجَنَّتُ نَعِیمࣲ" , "tr" : "(ለእርሱ) ዕረፍት፣ መልካም ሲሳይም፣ የመጠቀሚያ ገነትም አልለው፡፡" },
{ "chapter" : 56 , "verse" : 90 , "ar" : "وَأَمَّاۤ إِن كَانَ مِنۡ أَصۡحَـٰبِ ٱلۡیَمِینِ" , "tr" : "ከቀኝ ጓዶችም ቢኾንማ፡፡" },
{ "chapter" : 56 , "verse" : 91 , "ar" : "فَسَلَـٰمࣱ لَّكَ مِنۡ أَصۡحَـٰبِ ٱلۡیَمِینِ" , "tr" : "ከቀኝ ጓዶች ስለ ኾንክ ላንተ ሰላም አልለህ (ይባላል)፡፡" },
{ "chapter" : 56 , "verse" : 92 , "ar" : "وَأَمَّاۤ إِن كَانَ مِنَ ٱلۡمُكَذِّبِینَ ٱلضَّاۤلِّینَ" , "tr" : "ከሚያስዋሹት ጠማማዎችም ቢኾንማ፡፡" },
{ "chapter" : 56 , "verse" : 93 , "ar" : "فَنُزُلࣱ مِّنۡ حَمِیمࣲ" , "tr" : "ከፈላ ውሃ የኾነ መስተንግዶ አልለው፡፡" },
{ "chapter" : 56 , "verse" : 94 , "ar" : "وَتَصۡلِیَةُ جَحِیمٍ" , "tr" : "በገሀነም መቃጠልም (አልለው)፡፡" },
{ "chapter" : 56 , "verse" : 95 , "ar" : "إِنَّ هَـٰذَا لَهُوَ حَقُّ ٱلۡیَقِینِ" , "tr" : "ይህ እርሱ እርግጠኛ እውነት ነው፡፡" },
{ "chapter" : 56 , "verse" : 96 , "ar" : "فَسَبِّحۡ بِٱسۡمِ رَبِّكَ ٱلۡعَظِیمِ" , "tr" : "የታላቁንም ጌታህን ስም አወድስ፡፡" },
{ "chapter" : 57 , "verse" : 1 , "ar" : "سَبَّحَ لِلَّهِ مَا فِی ٱلسَّمَـٰوَ ٰتِ وَٱلۡأَرۡضِۖ وَهُوَ ٱلۡعَزِیزُ ٱلۡحَكِیمُ" , "tr" : "በሰማያትና በምድር ያለው ሁሉ ለአላህ አሞገሰ፡፡ እርሱም ሁሉን አሸናፊው ጥበበኛው ነው፡፡" },
{ "chapter" : 57 , "verse" : 2 , "ar" : "لَهُۥ مُلۡكُ ٱلسَّمَـٰوَ ٰتِ وَٱلۡأَرۡضِۖ یُحۡیِۦ وَیُمِیتُۖ وَهُوَ عَلَىٰ كُلِّ شَیۡءࣲ قَدِیرٌ" , "tr" : "የሰማያትና የምድር ንግሥና የእርሱ ብቻ ነው፡፡ ሕያው ያደርጋል፤ ይገድላልም፡፡ እርሱም በነገሩ ሁሉ ላይ ቻይ ነው፡፡" },
{ "chapter" : 57 , "verse" : 3 , "ar" : "هُوَ ٱلۡأَوَّلُ وَٱلۡءَاخِرُ وَٱلظَّـٰهِرُ وَٱلۡبَاطِنُۖ وَهُوَ بِكُلِّ شَیۡءٍ عَلِیمٌ" , "tr" : "እርሱ ፊትም ያለ፣ ኋላም ቀሪ፣ ግልጽም፣ ስውርም ነው፡፡ እርሱም ነገሩን ሁሉ ዐዋቂ ነው፡፡" }
] }