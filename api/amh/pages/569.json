{"pages" : [ 
{ "chapter" : 70 , "verse" : 11 , "ar" : "یُبَصَّرُونَهُمۡۚ یَوَدُّ ٱلۡمُجۡرِمُ لَوۡ یَفۡتَدِی مِنۡ عَذَابِ یَوۡمِئِذِۭ بِبَنِیهِ" , "tr" : "(ዘመዶቻቸውን) ያዩዋቸዋል፡፡ አመጸኛው ከዚያ ቀን ስቃይ (ነፍሱን) በልጆቹ ሊበዥ ይመኛል፡፡" },
{ "chapter" : 70 , "verse" : 12 , "ar" : "وَصَـٰحِبَتِهِۦ وَأَخِیهِ" , "tr" : "በሚስቱም በወንድሙም፡፡" },
{ "chapter" : 70 , "verse" : 13 , "ar" : "وَفَصِیلَتِهِ ٱلَّتِی تُءۡوِیهِ" , "tr" : "በዚያችም በምታስጠጋው ጎሳው፡፡" },
{ "chapter" : 70 , "verse" : 14 , "ar" : "وَمَن فِی ٱلۡأَرۡضِ جَمِیعࣰا ثُمَّ یُنجِیهِ" , "tr" : "በምድርም ላይ ባለው ሁሉ (ሊበዥ) ከዚያም ሊያድነው (ይመኛል)፡፡" },
{ "chapter" : 70 , "verse" : 15 , "ar" : "كَلَّاۤۖ إِنَّهَا لَظَىٰ" , "tr" : "ይተው! እርሷ (እሳቲቱ) ለዟ ናት፡፡" },
{ "chapter" : 70 , "verse" : 16 , "ar" : "نَزَّاعَةࣰ لِّلشَّوَىٰ" , "tr" : "የራስን ቅል ቆዳ የምትሞሽልቅ ስትኾን፡፡" },
{ "chapter" : 70 , "verse" : 17 , "ar" : "تَدۡعُوا۟ مَنۡ أَدۡبَرَ وَتَوَلَّىٰ" , "tr" : "(ከእምነት) የዞረንና የሸሸን ሰው (ወደእርሷ) ትጠራለች፡፡" },
{ "chapter" : 70 , "verse" : 18 , "ar" : "وَجَمَعَ فَأَوۡعَىٰۤ" , "tr" : "ገንዘብን የሰበሰበንና (ዘካውን ሳይሰጥ) የቆጠረንም፤ (ትጠራለች)፡፡" },
{ "chapter" : 70 , "verse" : 19 , "ar" : "۞ إِنَّ ٱلۡإِنسَـٰنَ خُلِقَ هَلُوعًا" , "tr" : "ሰው ቅጠ ቢስ ኾኖ ተፈጠረ፡፡" },
{ "chapter" : 70 , "verse" : 20 , "ar" : "إِذَا مَسَّهُ ٱلشَّرُّ جَزُوعࣰا" , "tr" : "ክፉ ነገር ባገኘው ጊዜ ብስጪተኛ፡፡" },
{ "chapter" : 70 , "verse" : 21 , "ar" : "وَإِذَا مَسَّهُ ٱلۡخَیۡرُ مَنُوعًا" , "tr" : "መልካምም ነገር (ድሎት) ባገኘ ጊዜ ከልካይ (ኾኖ ተፈጠረ)፡፡" },
{ "chapter" : 70 , "verse" : 22 , "ar" : "إِلَّا ٱلۡمُصَلِّینَ" , "tr" : "ሰጋጆቹ ብቻ ሲቀሩ፡፡" },
{ "chapter" : 70 , "verse" : 23 , "ar" : "ٱلَّذِینَ هُمۡ عَلَىٰ صَلَاتِهِمۡ دَاۤئِمُونَ" , "tr" : "እነዚያ እነርሱ በስግደታቸው ላይ ዘውታሪዎች የኾኑት፡፡" },
{ "chapter" : 70 , "verse" : 24 , "ar" : "وَٱلَّذِینَ فِیۤ أَمۡوَ ٰلِهِمۡ حَقࣱّ مَّعۡلُومࣱ" , "tr" : "እነዚያም በገንዘቦቻቸው ላይ የታወቀ መብት ያለባቸው የኾኑት፡፡" },
{ "chapter" : 70 , "verse" : 25 , "ar" : "لِّلسَّاۤئِلِ وَٱلۡمَحۡرُومِ" , "tr" : "ለለማኝ ከልመና ለሚከለከልም (መብት ያለባቸው የኾኑት)፡፡" },
{ "chapter" : 70 , "verse" : 26 , "ar" : "وَٱلَّذِینَ یُصَدِّقُونَ بِیَوۡمِ ٱلدِّینِ" , "tr" : "እነዚያም በፍርዱ ቀን እውነት የሚሉት (የሚያረጋግጡት)፡፡" },
{ "chapter" : 70 , "verse" : 27 , "ar" : "وَٱلَّذِینَ هُم مِّنۡ عَذَابِ رَبِّهِم مُّشۡفِقُونَ" , "tr" : "እነዚያም እነርሱ ከጌታቸው ቅጣት ፈሪዎች የኾኑት፡፡" },
{ "chapter" : 70 , "verse" : 28 , "ar" : "إِنَّ عَذَابَ رَبِّهِمۡ غَیۡرُ مَأۡمُونࣲ" , "tr" : "የጌታቸው ቅጣት (መምጣቱ) የማያስተማምን ነውና፡፡" },
{ "chapter" : 70 , "verse" : 29 , "ar" : "وَٱلَّذِینَ هُمۡ لِفُرُوجِهِمۡ حَـٰفِظُونَ" , "tr" : "እነዚያም እነርሱ ብልቶቻቸውን ጠባቂዎች የኾኑት፡፡" },
{ "chapter" : 70 , "verse" : 30 , "ar" : "إِلَّا عَلَىٰۤ أَزۡوَ ٰجِهِمۡ أَوۡ مَا مَلَكَتۡ أَیۡمَـٰنُهُمۡ فَإِنَّهُمۡ غَیۡرُ مَلُومِینَ" , "tr" : "በሚስቶቻቸው ወይም እጆቻቸው በያዙዋቸው (ባሪያዎች) ላይ ሲቀር፡፡ እነርሱ (በእነዚህ) የማይወቀሱ ናቸውና፡፡" },
{ "chapter" : 70 , "verse" : 31 , "ar" : "فَمَنِ ٱبۡتَغَىٰ وَرَاۤءَ ذَ ٰلِكَ فَأُو۟لَـٰۤئِكَ هُمُ ٱلۡعَادُونَ" , "tr" : "ከዚያም ወዲያ የፈለገ ሰው እነዚያ እነሱ ድንበር አላፊዎች ናቸው፡፡" },
{ "chapter" : 70 , "verse" : 32 , "ar" : "وَٱلَّذِینَ هُمۡ لِأَمَـٰنَـٰتِهِمۡ وَعَهۡدِهِمۡ رَ ٰعُونَ" , "tr" : "እነዚያም እነርሱ አደራዎቻቸውንና ቃል ኪዳናቸውን ጠባቂዎች የኾኑት፡፡" },
{ "chapter" : 70 , "verse" : 33 , "ar" : "وَٱلَّذِینَ هُم بِشَهَـٰدَ ٰتِهِمۡ قَاۤئِمُونَ" , "tr" : "እነዚያም እነርሱ በምስክርነታቸው ትክክለኞች የኾኑት፡፡" },
{ "chapter" : 70 , "verse" : 34 , "ar" : "وَٱلَّذِینَ هُمۡ عَلَىٰ صَلَاتِهِمۡ یُحَافِظُونَ" , "tr" : "እነዚያም እነርሱ በሶላቶቻቸው ላይ የሚጠባበቁት፡፡" },
{ "chapter" : 70 , "verse" : 35 , "ar" : "أُو۟لَـٰۤئِكَ فِی جَنَّـٰتࣲ مُّكۡرَمُونَ" , "tr" : "እነዚህ (ከዚህ በላይ የተወሱት ሁሉ) በገነቶች ውስጥ የሚከበሩ ናቸው፡፡" },
{ "chapter" : 70 , "verse" : 36 , "ar" : "فَمَالِ ٱلَّذِینَ كَفَرُوا۟ قِبَلَكَ مُهۡطِعِینَ" , "tr" : "ለእነዚያም ለካዱት ወደ አንተ በኩል አንገቶቻቸውን መዝዘው የሚያተኩሩት ምን አላቸው" },
{ "chapter" : 70 , "verse" : 37 , "ar" : "عَنِ ٱلۡیَمِینِ وَعَنِ ٱلشِّمَالِ عِزِینَ" , "tr" : "ከቀኝና ከግራ ክፍልፍል ጭፍሮች ሲኾኑ፡፡" },
{ "chapter" : 70 , "verse" : 38 , "ar" : "أَیَطۡمَعُ كُلُّ ٱمۡرِئࣲ مِّنۡهُمۡ أَن یُدۡخَلَ جَنَّةَ نَعِیمࣲ" , "tr" : "ከእነርሱ እያንዳንዱ ሰው የጸጋይቱን ገነት መግባትን ይከጅላልን" },
{ "chapter" : 70 , "verse" : 39 , "ar" : "كَلَّاۤۖ إِنَّا خَلَقۡنَـٰهُم مِّمَّا یَعۡلَمُونَ" , "tr" : "ይከልከል፤ እኛ ከሚያውቁት ነገር (ከፍቶት ጠብታ) ፈጠርናቸው፡፡" }
] }