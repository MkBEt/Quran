{"pages" : [ 
{ "chapter" : 74 , "verse" : 48 , "ar" : "فَمَا تَنفَعُهُمۡ شَفَـٰعَةُ ٱلشَّـٰفِعِینَ" , "tr" : "የአማላጆችም ምልጃ አትጠቅማቸውም፡፡" },
{ "chapter" : 74 , "verse" : 49 , "ar" : "فَمَا لَهُمۡ عَنِ ٱلتَّذۡكِرَةِ مُعۡرِضِینَ" , "tr" : "ከግሣጼዬም (ከቁርኣን) የሚያፈገፍጉት ለእነርሱ ምን አልላቸው" },
{ "chapter" : 74 , "verse" : 50 , "ar" : "كَأَنَّهُمۡ حُمُرࣱ مُّسۡتَنفِرَةࣱ" , "tr" : "እነርሱ ልክ ደንባሪዎች የሜዳ አህዮች ይመስላሉ፡፡" },
{ "chapter" : 74 , "verse" : 51 , "ar" : "فَرَّتۡ مِن قَسۡوَرَةِۭ" , "tr" : "ከዐንበሳ የሸሹ፡፡" },
{ "chapter" : 74 , "verse" : 52 , "ar" : "بَلۡ یُرِیدُ كُلُّ ٱمۡرِئࣲ مِّنۡهُمۡ أَن یُؤۡتَىٰ صُحُفࣰا مُّنَشَّرَةࣰ" , "tr" : "ይልቁንም ከእነርሱ እያንዳንዱ ሰው ሁሉ የተዘረጉ ጽሑፎችን እንዲስሰጥ ይፈልጋል፡፡" },
{ "chapter" : 74 , "verse" : 53 , "ar" : "كَلَّاۖ بَل لَّا یَخَافُونَ ٱلۡءَاخِرَةَ" , "tr" : "ይከልከሉ፡፡ ይልቁንም የመጨረሻይቱን ዓለም (ቅጣት) አይፈሩም፡፡" },
{ "chapter" : 74 , "verse" : 54 , "ar" : "كَلَّاۤ إِنَّهُۥ تَذۡكِرَةࣱ" , "tr" : "ይከልከሉ፡፡ እርሱ (ቁርኣን) መገሠጫ ነው፡፡" },
{ "chapter" : 74 , "verse" : 55 , "ar" : "فَمَن شَاۤءَ ذَكَرَهُۥ" , "tr" : "ስለዚህ የሻ ሰው ይገሰጽበታል፡፡" },
{ "chapter" : 74 , "verse" : 56 , "ar" : "وَمَا یَذۡكُرُونَ إِلَّاۤ أَن یَشَاۤءَ ٱللَّهُۚ هُوَ أَهۡلُ ٱلتَّقۡوَىٰ وَأَهۡلُ ٱلۡمَغۡفِرَةِ" , "tr" : "አላህም ካልሻ በስተቀር አይገሠጹም፡፡ እርሱ (አላህ) የመፈራት ባለቤት የምሕረትም ባለቤት ነው፡፡" },
{ "chapter" : 75 , "verse" : 1 , "ar" : "۞ لَاۤ أُقۡسِمُ بِیَوۡمِ ٱلۡقِیَـٰمَةِ" , "tr" : "(ነገሩ ከሓዲዎች እንደሚሉት) አይደለም፡፡ በትንሣኤ ቀን እምላለሁ፡፡" },
{ "chapter" : 75 , "verse" : 2 , "ar" : "وَلَاۤ أُقۡسِمُ بِٱلنَّفۡسِ ٱللَّوَّامَةِ" , "tr" : "(ራሷን) ወቃሽ በኾነች ነፍስም እምላለሁ፡፡ (በእርግጥ ትቀሰቀሳላችሁ)፡፡" },
{ "chapter" : 75 , "verse" : 3 , "ar" : "أَیَحۡسَبُ ٱلۡإِنسَـٰنُ أَلَّن نَّجۡمَعَ عِظَامَهُۥ" , "tr" : "ሰው አጥንቶቹን አለመሰብሰባችንን ያስባልን" },
{ "chapter" : 75 , "verse" : 4 , "ar" : "بَلَىٰ قَـٰدِرِینَ عَلَىٰۤ أَن نُّسَوِّیَ بَنَانَهُۥ" , "tr" : "አይደለም ጣቶቹን (ፊት እንደ ነበሩ) በማስተካከል ላይ ቻዮች ስንኾን (እንሰበስባቸዋለን)፡፡" },
{ "chapter" : 75 , "verse" : 5 , "ar" : "بَلۡ یُرِیدُ ٱلۡإِنسَـٰنُ لِیَفۡجُرَ أَمَامَهُۥ" , "tr" : "ይልቁንም ሰው በፊቱ ባለው ነገር (በትንሣኤ) ሊያስተባብል ይፈልጋል፡፡" },
{ "chapter" : 75 , "verse" : 6 , "ar" : "یَسۡءَلُ أَیَّانَ یَوۡمُ ٱلۡقِیَـٰمَةِ" , "tr" : "«የትንሣኤው ቀን መቼ ነው?» ሲል ይጠይቃል፡፡" },
{ "chapter" : 75 , "verse" : 7 , "ar" : "فَإِذَا بَرِقَ ٱلۡبَصَرُ" , "tr" : "ዓይንም በዋለለ ጊዜ፡፡" },
{ "chapter" : 75 , "verse" : 8 , "ar" : "وَخَسَفَ ٱلۡقَمَرُ" , "tr" : "ጨረቃም በጨለመ ጊዜ፡፡" },
{ "chapter" : 75 , "verse" : 9 , "ar" : "وَجُمِعَ ٱلشَّمۡسُ وَٱلۡقَمَرُ" , "tr" : "ፀሐይና ጨረቃም በተሰበሰቡ ጊዜ፡፡" },
{ "chapter" : 75 , "verse" : 10 , "ar" : "یَقُولُ ٱلۡإِنسَـٰنُ یَوۡمَئِذٍ أَیۡنَ ٱلۡمَفَرُّ" , "tr" : "«ሰው በዚያ ቀን መሸሻው የት ነው?» ይላል፡፡" },
{ "chapter" : 75 , "verse" : 11 , "ar" : "كَلَّا لَا وَزَرَ" , "tr" : "ይከልከል፤ ምንም መጠጊያ የለም፡፡" },
{ "chapter" : 75 , "verse" : 12 , "ar" : "إِلَىٰ رَبِّكَ یَوۡمَئِذٍ ٱلۡمُسۡتَقَرُّ" , "tr" : "በዚያ ቀን መርጊያው ወደ ጌታህ ብቻ ነው፡፡" },
{ "chapter" : 75 , "verse" : 13 , "ar" : "یُنَبَّؤُا۟ ٱلۡإِنسَـٰنُ یَوۡمَئِذِۭ بِمَا قَدَّمَ وَأَخَّرَ" , "tr" : "ሰው በዚያ ቀን ባስቀደመውና ባስቆየው ሁሉ ይነገራል፡፡" },
{ "chapter" : 75 , "verse" : 14 , "ar" : "بَلِ ٱلۡإِنسَـٰنُ عَلَىٰ نَفۡسِهِۦ بَصِیرَةࣱ" , "tr" : "በእርግጥም ሰው በነፍሱ ላይ አስረጅ ነው፡፡ (አካሎቹ ይመሰክሩበታል)፡፡" },
{ "chapter" : 75 , "verse" : 15 , "ar" : "وَلَوۡ أَلۡقَىٰ مَعَاذِیرَهُۥ" , "tr" : "ምክንያቶቹን ሁሉ ቢያመጣም እንኳ (አይሰማም)፡፡" },
{ "chapter" : 75 , "verse" : 16 , "ar" : "لَا تُحَرِّكۡ بِهِۦ لِسَانَكَ لِتَعۡجَلَ بِهِۦۤ" , "tr" : "በእርሱ (በቁርኣን ንባብ) ልትቸኩል ምላስህን በእርሱ አታላውስ፡፡" },
{ "chapter" : 75 , "verse" : 17 , "ar" : "إِنَّ عَلَیۡنَا جَمۡعَهُۥ وَقُرۡءَانَهُۥ" , "tr" : "(በልብህ ውስጥ) መሰብሰቡና ማንበቡ በእኛ ላይ ነውና፡፡" },
{ "chapter" : 75 , "verse" : 18 , "ar" : "فَإِذَا قَرَأۡنَـٰهُ فَٱتَّبِعۡ قُرۡءَانَهُۥ" , "tr" : "ባነበብነውም ጊዜ (ካለቀ በኋላ) ንባቡን ተከተል፡፡" },
{ "chapter" : 75 , "verse" : 19 , "ar" : "ثُمَّ إِنَّ عَلَیۡنَا بَیَانَهُۥ" , "tr" : "ከዚያም ማብራራቱ በእኛ ላይ ነው፡፡" }
] }