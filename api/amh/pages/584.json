{"pages" : [ 
{ "chapter" : 79 , "verse" : 16 , "ar" : "إِذۡ نَادَىٰهُ رَبُّهُۥ بِٱلۡوَادِ ٱلۡمُقَدَّسِ طُوًى" , "tr" : "ጌታው በተቀደሰው ሸለቆ በጡዋ በጠራው ጊዜ፤" },
{ "chapter" : 79 , "verse" : 17 , "ar" : "ٱذۡهَبۡ إِلَىٰ فِرۡعَوۡنَ إِنَّهُۥ طَغَىٰ" , "tr" : "ወደ ፈርዖን ኺድ፡፡ እርሱ ወሰን አልፏልና፡፡" },
{ "chapter" : 79 , "verse" : 18 , "ar" : "فَقُلۡ هَل لَّكَ إِلَىٰۤ أَن تَزَكَّىٰ" , "tr" : "በለውም፡- «ወደ መጥራራትህ ላንተ (መንገድ) አለህን?»" },
{ "chapter" : 79 , "verse" : 19 , "ar" : "وَأَهۡدِیَكَ إِلَىٰ رَبِّكَ فَتَخۡشَىٰ" , "tr" : "«ወደ ጌታህም ልመራህ ትፈራውም ዘንድ፤ (መንገድ አለህን?)» አለው፡፡" },
{ "chapter" : 79 , "verse" : 20 , "ar" : "فَأَرَىٰهُ ٱلۡءَایَةَ ٱلۡكُبۡرَىٰ" , "tr" : "ታላቂቱንም ተዓምር አሳየው፡፡" },
{ "chapter" : 79 , "verse" : 21 , "ar" : "فَكَذَّبَ وَعَصَىٰ" , "tr" : "አስተባበለም፤ አመጸም፡፡" },
{ "chapter" : 79 , "verse" : 22 , "ar" : "ثُمَّ أَدۡبَرَ یَسۡعَىٰ" , "tr" : "ከዚያም (ለማጥፋት) የሚተጋ ኾኖ ዞረ፡፡" },
{ "chapter" : 79 , "verse" : 23 , "ar" : "فَحَشَرَ فَنَادَىٰ" , "tr" : "(ሰራዊቶቹን) ሰበሰበም፤ ተጣራም፡፡" },
{ "chapter" : 79 , "verse" : 24 , "ar" : "فَقَالَ أَنَا۠ رَبُّكُمُ ٱلۡأَعۡلَىٰ" , "tr" : "አለም፡- «እኔ ታላቁ ጌታችሁ ነኝ፡፡»" },
{ "chapter" : 79 , "verse" : 25 , "ar" : "فَأَخَذَهُ ٱللَّهُ نَكَالَ ٱلۡءَاخِرَةِ وَٱلۡأُولَىٰۤ" , "tr" : "አላህም በኋለኛይቱና በፊተኛይቱ (ቃል) ቅጣት ያዘው፡፡" },
{ "chapter" : 79 , "verse" : 26 , "ar" : "إِنَّ فِی ذَ ٰلِكَ لَعِبۡرَةࣰ لِّمَن یَخۡشَىٰۤ" , "tr" : "በዚህ ውስጥ ለሚፈራ ሰው በእርግጥ መገምገሚያ አለበት፡፡" },
{ "chapter" : 79 , "verse" : 27 , "ar" : "ءَأَنتُمۡ أَشَدُّ خَلۡقًا أَمِ ٱلسَّمَاۤءُۚ بَنَىٰهَا" , "tr" : "ባፈጣጠር እናንተ ይበልጥ የበረታችሁ ናችሁን? ወይንስ ሰማይ? (አላህ) ገነባት፡፡" },
{ "chapter" : 79 , "verse" : 28 , "ar" : "رَفَعَ سَمۡكَهَا فَسَوَّىٰهَا" , "tr" : "ከፍታዋን አጓነ፤ አስተካከላትም፡፡" },
{ "chapter" : 79 , "verse" : 29 , "ar" : "وَأَغۡطَشَ لَیۡلَهَا وَأَخۡرَجَ ضُحَىٰهَا" , "tr" : "ሌሊቷንም አጨለመ፡፡ ቀንዋንም ገለጸ፡፡" },
{ "chapter" : 79 , "verse" : 30 , "ar" : "وَٱلۡأَرۡضَ بَعۡدَ ذَ ٰلِكَ دَحَىٰهَاۤ" , "tr" : "ምድርንም ከዚህ በኋላ ዘረጋት፡፡" },
{ "chapter" : 79 , "verse" : 31 , "ar" : "أَخۡرَجَ مِنۡهَا مَاۤءَهَا وَمَرۡعَىٰهَا" , "tr" : "ውሃዋንና ግጦሿን ከእርሷ አወጣ፡፡" },
{ "chapter" : 79 , "verse" : 32 , "ar" : "وَٱلۡجِبَالَ أَرۡسَىٰهَا" , "tr" : "ጋራዎችንም አደላደላቸው፡፡" },
{ "chapter" : 79 , "verse" : 33 , "ar" : "مَتَـٰعࣰا لَّكُمۡ وَلِأَنۡعَـٰمِكُمۡ" , "tr" : "ለእናንተና ለእንስሶቻችሁ መጣቀሚያ ይኾን ዘንድ (ይህን አደረገ)፡፡" },
{ "chapter" : 79 , "verse" : 34 , "ar" : "فَإِذَا جَاۤءَتِ ٱلطَّاۤمَّةُ ٱلۡكُبۡرَىٰ" , "tr" : "ታላቂቱም መዐት በመጣች ጊዜ፣" },
{ "chapter" : 79 , "verse" : 35 , "ar" : "یَوۡمَ یَتَذَكَّرُ ٱلۡإِنسَـٰنُ مَا سَعَىٰ" , "tr" : "ሰው ሁሉ የሠራውን በሚያስታውስበት ቀን፣" },
{ "chapter" : 79 , "verse" : 36 , "ar" : "وَبُرِّزَتِ ٱلۡجَحِیمُ لِمَن یَرَىٰ" , "tr" : "ገሀነምም ለሚያይ ሰው በተገለጠች ጊዜ፣" },
{ "chapter" : 79 , "verse" : 37 , "ar" : "فَأَمَّا مَن طَغَىٰ" , "tr" : "የካደ ሰውማ፣" },
{ "chapter" : 79 , "verse" : 38 , "ar" : "وَءَاثَرَ ٱلۡحَیَوٰةَ ٱلدُّنۡیَا" , "tr" : "ቅርቢቱንም ሕይወት የመረጠ፣" },
{ "chapter" : 79 , "verse" : 39 , "ar" : "فَإِنَّ ٱلۡجَحِیمَ هِیَ ٱلۡمَأۡوَىٰ" , "tr" : "ገሀነም እርሷ በእርግጥ መኖሪያው ናት፡፡" },
{ "chapter" : 79 , "verse" : 40 , "ar" : "وَأَمَّا مَنۡ خَافَ مَقَامَ رَبِّهِۦ وَنَهَى ٱلنَّفۡسَ عَنِ ٱلۡهَوَىٰ" , "tr" : "በጌታው ፊት መቆምን የፈራ ሰውማ ነፍሱንም ከዝንባሌዋ የከለከለ" },
{ "chapter" : 79 , "verse" : 41 , "ar" : "فَإِنَّ ٱلۡجَنَّةَ هِیَ ٱلۡمَأۡوَىٰ" , "tr" : "ገነት እርሷ በእርግጥ መኖሪያው ናት፡፡" },
{ "chapter" : 79 , "verse" : 42 , "ar" : "یَسۡءَلُونَكَ عَنِ ٱلسَّاعَةِ أَیَّانَ مُرۡسَىٰهَا" , "tr" : "«ከሰዓቲቱ መቼ ነው መኾኛዋ?» ሲሉ ይጠይቁሃል፡፡" },
{ "chapter" : 79 , "verse" : 43 , "ar" : "فِیمَ أَنتَ مِن ذِكۡرَىٰهَاۤ" , "tr" : "አንተ እርሷን ከማስታወስ በምን ውስጥ ነህ" },
{ "chapter" : 79 , "verse" : 44 , "ar" : "إِلَىٰ رَبِّكَ مُنتَهَىٰهَاۤ" , "tr" : "(የዕውቀት) መጨረሻዋ ወደ ጌታህ ብቻ ነው፡፡" },
{ "chapter" : 79 , "verse" : 45 , "ar" : "إِنَّمَاۤ أَنتَ مُنذِرُ مَن یَخۡشَىٰهَا" , "tr" : "አንተ የሚፈራትን ሰው አስፈራሪ ብቻ ነህ፡፡" },
{ "chapter" : 79 , "verse" : 46 , "ar" : "كَأَنَّهُمۡ یَوۡمَ یَرَوۡنَهَا لَمۡ یَلۡبَثُوۤا۟ إِلَّا عَشِیَّةً أَوۡ ضُحَىٰهَا" , "tr" : "እነርሱ በሚያዩዋት ቀን ያንዲት ቀን ምሽትን ወይም ረፏዷን እንጅ ያልቆዩ ይመስላሉ፡፡" }
] }