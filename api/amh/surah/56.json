{ "verse" : 57 , "ar" : "نَحۡنُ خَلَقۡنَـٰكُمۡ فَلَوۡلَا تُصَدِّقُونَ" , "tr" : "እኛ ፈጠርናችሁ አታምኑምን" },
{ "verse" : 58 , "ar" : "أَفَرَءَیۡتُم مَّا تُمۡنُونَ" , "tr" : "(በማኅፀኖች) የምታፈሱትን አያችሁን" },
{ "verse" : 59 , "ar" : "ءَأَنتُمۡ تَخۡلُقُونَهُۥۤ أَمۡ نَحۡنُ ٱلۡخَـٰلِقُونَ" , "tr" : "እናንተ ትፈጥሩታላችሁን? ወይስ እኛ ፈጣሪዎቹን ነን" },
{ "verse" : 60 , "ar" : "نَحۡنُ قَدَّرۡنَا بَیۡنَكُمُ ٱلۡمَوۡتَ وَمَا نَحۡنُ بِمَسۡبُوقِینَ" , "tr" : "እኛ ሞትን (ጊዜውን) በመካከላችሁ ወሰንን፡፡ እኛም ተሸናፊዎች አይደለንም፡፡" },
{ "verse" : 61 , "ar" : "عَلَىٰۤ أَن نُّبَدِّلَ أَمۡثَـٰلَكُمۡ وَنُنشِئَكُمۡ فِی مَا لَا تَعۡلَمُونَ" , "tr" : "ብጤዎቻችሁን በመለወጥና በዚያ በማታውቁትም (ቅርጽ) እናንተን በመፍጠር ላይ (አንሸነፍም)፡፡" },
{ "verse" : 62 , "ar" : "وَلَقَدۡ عَلِمۡتُمُ ٱلنَّشۡأَةَ ٱلۡأُولَىٰ فَلَوۡلَا تَذَكَّرُونَ" , "tr" : "የፊተኛይቱንም አፈጣጠር በእርግጥ ዐውቃችኋል፡፡ አትገነዘቡምን" },
{ "verse" : 63 , "ar" : "أَفَرَءَیۡتُم مَّا تَحۡرُثُونَ" , "tr" : "የምትዘሩትንም አያችሁን" },
{ "verse" : 64 , "ar" : "ءَأَنتُمۡ تَزۡرَعُونَهُۥۤ أَمۡ نَحۡنُ ٱلزَّ ٰرِعُونَ" , "tr" : "እናንተ ታበቅሉታላችሁን? ወይስ እኛ አብቃዮቹ ነን" },
{ "verse" : 65 , "ar" : "لَوۡ نَشَاۤءُ لَجَعَلۡنَـٰهُ حُطَـٰمࣰا فَظَلۡتُمۡ تَفَكَّهُونَ" , "tr" : "ብነሻ ኖሮ ደረቅ ስብርብር ባደረግነውና የምትደነቁም በኾናችሁ ነበር፡፡" },
{ "verse" : 66 , "ar" : "إِنَّا لَمُغۡرَمُونَ" , "tr" : "«እኛ በዕዳ ተያዦች ነን፡፡" },
{ "verse" : 67 , "ar" : "بَلۡ نَحۡنُ مَحۡرُومُونَ" , "tr" : "«በእርግጥም እኛ (ከመጠቀም) የተከለከልን ነን» (ትሉ ነበር)፡፡" },
{ "verse" : 68 , "ar" : "أَفَرَءَیۡتُمُ ٱلۡمَاۤءَ ٱلَّذِی تَشۡرَبُونَ" , "tr" : "ያንንም የምትጠጡትን ውሃ አየችሁን" },
{ "verse" : 69 , "ar" : "ءَأَنتُمۡ أَنزَلۡتُمُوهُ مِنَ ٱلۡمُزۡنِ أَمۡ نَحۡنُ ٱلۡمُنزِلُونَ" , "tr" : "እናንተ ከደመናው አወረዳችሁትን? ወይስ እኛ አውራጆቹ ነን" },
{ "verse" : 70 , "ar" : "لَوۡ نَشَاۤءُ جَعَلۡنَـٰهُ أُجَاجࣰا فَلَوۡلَا تَشۡكُرُونَ" , "tr" : "ብንሻ ኖሮ መርጋጋ ባደረግነው ነበር፡፡ አታመሰግኑምን" },
{ "verse" : 71 , "ar" : "أَفَرَءَیۡتُمُ ٱلنَّارَ ٱلَّتِی تُورُونَ" , "tr" : "ያቺንም የምትሰብቋትን እሳት አያችሁን" },
{ "verse" : 72 , "ar" : "ءَأَنتُمۡ أَنشَأۡتُمۡ شَجَرَتَهَاۤ أَمۡ نَحۡنُ ٱلۡمُنشِءُونَ" , "tr" : "እናንተ ዛፏን ፈጠራችሁን? ወይስ እኛ ፈጣሪዎችዋ ነን" },
{ "verse" : 73 , "ar" : "نَحۡنُ جَعَلۡنَـٰهَا تَذۡكِرَةࣰ وَمَتَـٰعࣰا لِّلۡمُقۡوِینَ" , "tr" : "እኛ (ለገሀናም) ማስታወሻ ለመንገደኞችም መጠቀሚያ አደረግናት፡፡" },
{ "verse" : 74 , "ar" : "فَسَبِّحۡ بِٱسۡمِ رَبِّكَ ٱلۡعَظِیمِ" , "tr" : "የታላቁን ጌታህንም ስም አወድስ፡፡" },
{ "verse" : 75 , "ar" : "۞ فَلَاۤ أُقۡسِمُ بِمَوَ ٰقِعِ ٱلنُّجُومِ" , "tr" : "በከዋክብትም መጥለቂያዎች እምላለሁ፡፡" },
{ "verse" : 76 , "ar" : "وَإِنَّهُۥ لَقَسَمࣱ لَّوۡ تَعۡلَمُونَ عَظِیمٌ" , "tr" : "እርሱም ብታወቁ ታላቅ መሓላ ነው፡፡" },
{ "verse" : 77 , "ar" : "إِنَّهُۥ لَقُرۡءَانࣱ كَرِیمࣱ" , "tr" : "እርሱ የከበረ ቁርኣን ነው፡፡" },
{ "verse" : 78 , "ar" : "فِی كِتَـٰبࣲ مَّكۡنُونࣲ" , "tr" : "በተጠበቀ መጽሐፍ ውስጥ ነው፡፡" },
{ "verse" : 79 , "ar" : "لَّا یَمَسُّهُۥۤ إِلَّا ٱلۡمُطَهَّرُونَ" , "tr" : "የተጥራሩት እንጅ ሌላ አይነካውም፡፡" },
{ "verse" : 80 , "ar" : "تَنزِیلࣱ مِّن رَّبِّ ٱلۡعَـٰلَمِینَ" , "tr" : "ከዓለማት ጌታ የተወረደ ነው፡፡" },
{ "verse" : 81 , "ar" : "أَفَبِهَـٰذَا ٱلۡحَدِیثِ أَنتُم مُّدۡهِنُونَ" , "tr" : "በዚህ ንግግር እናንተ ቸልተኞች ናችሁን" },
{ "verse" : 82 , "ar" : "وَتَجۡعَلُونَ رِزۡقَكُمۡ أَنَّكُمۡ تُكَذِّبُونَ" , "tr" : "ሲሳያችሁንም (ዝናምን) እናንተ የምታስተባብሉት ታደርጋላችሁን" },
{ "verse" : 83 , "ar" : "فَلَوۡلَاۤ إِذَا بَلَغَتِ ٱلۡحُلۡقُومَ" , "tr" : "(ነፍስ) ጉረሮንም በደረሰች ጊዜ፡፡" },
{ "verse" : 84 , "ar" : "وَأَنتُمۡ حِینَئِذࣲ تَنظُرُونَ" , "tr" : "እናንተ ያን ጊዜ የምታዩ ስትኾኑ፡፡" },
{ "verse" : 85 , "ar" : "وَنَحۡنُ أَقۡرَبُ إِلَیۡهِ مِنكُمۡ وَلَـٰكِن لَّا تُبۡصِرُونَ" , "tr" : "እኛም ግን አታዩም እንጅ ከእናንተ ይልቅ ወደእርሱ የቀረብን ስንኾን፡፡" },
{ "verse" : 86 , "ar" : "فَلَوۡلَاۤ إِن كُنتُمۡ غَیۡرَ مَدِینِینَ" , "tr" : "የማትዳኙም ከኾናችሁ፡፡" },
{ "verse" : 87 , "ar" : "تَرۡجِعُونَهَاۤ إِن كُنتُمۡ صَـٰدِقِینَ" , "tr" : "እውነተኞች እንደ ኾናችሁ (ነፍሲቱን ወደ አካሉ) ለምን አትመልሷትም፡፡" },
{ "verse" : 88 , "ar" : "فَأَمَّاۤ إِن كَانَ مِنَ ٱلۡمُقَرَّبِینَ" , "tr" : "(ሟቹ) ከባለሟሎቹ ቢኾንማ፡፡" },
{ "verse" : 89 , "ar" : "فَرَوۡحࣱ وَرَیۡحَانࣱ وَجَنَّتُ نَعِیمࣲ" , "tr" : "(ለእርሱ) ዕረፍት፣ መልካም ሲሳይም፣ የመጠቀሚያ ገነትም አልለው፡፡" },
{ "verse" : 90 , "ar" : "وَأَمَّاۤ إِن كَانَ مِنۡ أَصۡحَـٰبِ ٱلۡیَمِینِ" , "tr" : "ከቀኝ ጓዶችም ቢኾንማ፡፡" },
{ "verse" : 91 , "ar" : "فَسَلَـٰمࣱ لَّكَ مِنۡ أَصۡحَـٰبِ ٱلۡیَمِینِ" , "tr" : "ከቀኝ ጓዶች ስለ ኾንክ ላንተ ሰላም አልለህ (ይባላል)፡፡" },
{ "verse" : 92 , "ar" : "وَأَمَّاۤ إِن كَانَ مِنَ ٱلۡمُكَذِّبِینَ ٱلضَّاۤلِّینَ" , "tr" : "ከሚያስዋሹት ጠማማዎችም ቢኾንማ፡፡" },
{ "verse" : 93 , "ar" : "فَنُزُلࣱ مِّنۡ حَمِیمࣲ" , "tr" : "ከፈላ ውሃ የኾነ መስተንግዶ አልለው፡፡" },
{ "verse" : 94 , "ar" : "وَتَصۡلِیَةُ جَحِیمٍ" , "tr" : "በገሀነም መቃጠልም (አልለው)፡፡" },
{ "verse" : 95 , "ar" : "إِنَّ هَـٰذَا لَهُوَ حَقُّ ٱلۡیَقِینِ" , "tr" : "ይህ እርሱ እርግጠኛ እውነት ነው፡፡" },
{ "verse" : 96 , "ar" : "فَسَبِّحۡ بِٱسۡمِ رَبِّكَ ٱلۡعَظِیمِ" , "tr" : "የታላቁንም ጌታህን ስም አወድስ፡፡" }
] }